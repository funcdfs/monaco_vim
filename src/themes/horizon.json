{
  "base": "vs-dark",
  "inherit": true,
  "rules": [
    {
      "token": "comment",
      "foreground": "6C6F93",
      "fontStyle": "italic"
    },
    {
      "token": "constant",
      "foreground": "F43E5C"
    },
    {
      "token": "keyword",
      "foreground": "B877DB"
    },
    {
      "token": "variable",
      "foreground": "E95678"
    },
    {
      "token": "string",
      "foreground": "FAB795"
    },
    {
      "token": "function",
      "foreground": "25B2BC"
    },
    {
      "token": "type",
      "foreground": "FAC29A"
    }
  ],
  "colors": {
    "editor.background": "#1C1E26",
    "editor.foreground": "#FDF0ED",
    "editor.lineHighlightBackground": "#232530",
